{
    "name": "enklare-development/laravel-gelf-logger",
    "type": "library",
    "description": "Fork of the hedii/laravel-gelf-logger project with extended support for HttpTransport",
    "keywords": ["laravel", "graylog", "gelf", "http", "enklare"],
    "homepage": "https://www.enklare.se",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Wahlberg",
            "email": "pierre.vahlberg@enklare.se",
            "homepage": "https://www.enklare.se",
            "role": "Developer"
        },
        {
            "name": "Christian Nilsson",
            "email": "christian.nilsson@enklare.se",
            "homepage": "https://www.enklare.se",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/enklare-development/laravel-gelf-logger/issues",
        "source": "https://github.com/enklare-development/laravel-gelf-logger"
    },
    "require": {
        "php": "^7.2",
        "illuminate/log": "^6",
        "graylog2/gelf-php": "^1.6"
    },
    "require-dev": {
        "php": "^7.2",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Hedii\\LaravelGelfLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hedii\\LaravelGelfLogger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
